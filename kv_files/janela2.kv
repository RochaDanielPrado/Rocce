    cols:1
    #md_bg_color: 0,1,1,1
    #pos_hint: {'right':1}
    spacing: '2dp'
    #padding: [10,0,20,0]

    MDGridLayout:
        cols:2
        size_hint_x: None
        pos_hint: {'center_x':.9}
        width: self.parent.width
        padding:10

        #md_bg_color: 0,1,1,.5
        MDLabel:
            id: label1
            text: ''
            size_hint_x:None
            width: 30
            md_bg_color: .9,.9,.9,.5
            theme_text_color: "Custom"
            text_color: 1, 1, 1, 1
            halign:'center'
            radius:10

        MDGridLayout:
            cols:1
            size_hint_x: None
            width: self.parent.width
            padding:0

            MDIconButton:
                icon: "plus-box-outline"
                size_hint_x: None
                width: self.parent.minimum_width
                halign:'left'
                on_release: root.n_plus()
            MDIconButton:
                icon: "minus-box-outline"
                halign:'left'
                size_hint_x: None
                width: self.parent.minimum_width
                on_release: root.n_minus()
